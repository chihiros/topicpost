{
  "name": "web",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "// build": "remix vite:build",
    "build": "npm run generate; remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "dev:css": "tailwindcss -w -i ./app/tailwind.css -o ./app/tailwind.generated.css",
    "cp": "cp -r ../docs/openapi ./app/services/openapi",
    "rm": "rm -rf ./app/services/openapi/openapi",
    "ogen": "openapi-generator-cli generate -i app/services/openapi/openapi/openapi.yml -g typescript-axios --additional-properties supportsES6=true,withInterfaces=true -o app/services/openapi/generated/",
    "generate": "npm run cp; npm run ogen; npm run rm;",
    "cpenv": "cp .env.example .env; cat .env.supabase.local >> .env"
  },
  "dependencies": {
    "@remix-run/node": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "@remix-run/serve": "^2.13.1",
    "@supabase/supabase-js": "^2.45.6",
    "@twemoji/api": "^15.1.0",
    "@twemoji/parser": "^15.1.1",
    "@vercel/remix": "^2.13.1",
    "axios": "^1.7.7",
    "cross-env": "^7.0.3",
    "isbot": "^4.4.0",
    "react": "19.0.0-beta-26f2496093-20240514",
    "react-dom": "19.0.0-beta-26f2496093-20240514",
    "react-icons": "^5.3.0",
    "react-toastify": "^10.0.6",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "@remix-run/dev": "^2.13.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/twemoji-parser": "^13.1.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": "20.x"
  }
}
